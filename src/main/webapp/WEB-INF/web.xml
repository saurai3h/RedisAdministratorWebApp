<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>Controller.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>listservers</servlet-name>
        <servlet-class>Controller.ListInstanceServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>initialpageservlet</servlet-name>
        <servlet-class>Controller.InitialPageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PreviousPage</servlet-name>
        <servlet-class>Controller.PreviousPageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NextPage</servlet-name>
        <servlet-class>Controller.NextPageServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/view/Log.in</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>listservers</servlet-name>
        <url-pattern>/view/RedisApplication</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>initialpageservlet</servlet-name>
        <url-pattern>/view/RedisApplication2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PreviousPage</servlet-name>
        <url-pattern>/view/Previous</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NextPage</servlet-name>
        <url-pattern>/view/Next</url-pattern>
    </servlet-mapping>
</web-app>